<div class="container">
	<div class="tagsWrapper">
		<button type="submit" id="updateData">Update graph data</button>
		<div class="filterDiv">
			<label for="filter">Write comma-seperated tags for filter</label>
			<input id="tagSelected-box" name="filter" type="text"  class="filterInputField" size="15px"></input>
		</div>
	<div id="placeholder"></div>
	<br>
	<div id="cal">
	  <div id="start-cal">
	    <div id="start-text">Start Date</div>
    	<div id="start-calendar-container"></div>
    	<input size="19px" id="start-calendar-input"></input>
    </div>
  	<div id="end-cal">
      <div id="end-text">End Date</div>
    	<div id="end-calendar-container"></div>
    	<input size="19px" id="end-calendar-input"></input>
    	</div>
  </div>
  </div>
</div>

